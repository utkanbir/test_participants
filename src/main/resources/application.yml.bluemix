server:
  port: 8181
eureka:
  client:
    serviceUrl:
      defaultZone: ${vcap.services.ocr-eureka.credentials.uri:http://ocr-eureka-tolga.mybluemix.net/eureka/}
  instance:
    hostname: ${vcap.application.uris[0]:localhost}
    metadataMap:
      instanceId: ${vcap.application.instance_id:${spring.application.name}:${spring.application.instance_id:${server.port}}}
      
---
spring:
  profiles: cloud
eureka:
  instance:
    nonSecurePort: 80